{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
        "max-line-length": {
            "options": [120]
        }
    },
    "rules": {
        "max-line-length": {
            "options": [120]
        },
        "semicolon": [true, "always"],
        "typedef": [true, "call-signature", "parameter", "member-variable-declaration"]
    },
    "rulesDirectory": [],
    "linterOptions": {
        "format": "codeFrame"
    }
}